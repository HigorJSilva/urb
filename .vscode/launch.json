{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Docker",
            "protocol": "inspector",
            "port": 9239,
            "restart": true,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app",
            "resolveSourceMapLocations": [
                "${workspaceFolder}/src",
                "!/node_modules/**"
            ],
        }
    ]
}